#include<bits/stdc++.h>

using namespace std;

int main()
{
	int A[]={1, 3, 5, 7};
	int B[]={0, 2, 6, 8, 9};
	int C[100];
	int m=sizeof(A)/sizeof(int);
	int n=sizeof(B)/sizeof(int);
	
	for(int i=0;i<m;i++)
	{
		C[i]=A[i];
	}	
		
	for(int j=0;j<n;j++)
	{
		C[m+j]=B[j];	
	}	
	
		int t=m+n;
	
	for(int i=0;i<t;i++)
	{
		cout<<C[i];
	}
	
	sort(C,C+t);
	for(int i=0;i<t;i++)
	{
		cout<<C[i];
	}	
}
